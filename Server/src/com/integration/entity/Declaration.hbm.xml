<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping
PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.integration.entity.Declaration" table="declaration">
		<id name="declare_id" column="declare_id" type="int">
			<generator class="identity"> <!-- 主键id的生成方式为自增 -->
			</generator>
		</id>
		<property name="customer_id" column="customer_id" type="int"></property>
		<property name="cash_num" column="cash_num" type="int"></property>
		<property name="pre_declare_id" column="pre_declare_id" type="int"></property>
	</class>
</hibernate-mapping>